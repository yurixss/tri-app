{
  "expo": {
    "name": "TriCalc Pro",
    "slug": "tricalc-pro",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/tri-icon.png",
    "scheme": "tricalcpro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.yurixss.tricalcpro",
      "buildNumber": "2",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.yurixss.tricalcpro"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bd3a21e0-9952-4caf-8510-8e253933bd2d"
      }
    }
  }
}
